{
  "name": "maltdecoder",
  "version": "1.0.0",
  "description": "Chrome plugin to decode SMWS codes on the UK and JP sites",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "webpack --env production",
    "ci": "webpack --env ci",
    "test": "nyc --reporter=text mocha test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elliottback/MaltDecoder.git"
  },
  "private": true,
  "keywords": [
    "scotch",
    "whisky",
    "society",
    "smws",
    "chrome"
  ],
  "author": "Elliott C. BÃ¤ck",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/elliottback/MaltDecoder/issues"
  },
  "homepage": "https://github.com/elliottback/MaltDecoder#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.25.2",
    "axios": "^1.7.2",
    "babel-loader": "^9.1.3",
    "chai": "^5.1.1",
    "copy-webpack-plugin": "^12.0.2",
    "jsdom": "^24.1.1",
    "lodash": "^4.17.21",
    "mocha": "^10.7.0",
    "nanoid": ">=5.0.7",
    "nyc": "^17.0.0",
    "puppeteer": "^22.15.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "zip-webpack-plugin": "^4.0.1"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/github",
        {
          "assets": [
            "dist/MaltDecoder.zip"
          ]
        }
      ]
    ]
  }
}
